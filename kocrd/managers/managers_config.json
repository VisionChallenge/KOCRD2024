  "DEFAULT_REPORT_FILENAME": "report.txt",
  "DEFAULT_EXCEL_FILENAME": "report.xlsx",
  "VALID_FILE_EXTENSIONS": [".txt", ".pdf", ".png", ".jpg"],
  "MAX_FILE_SIZE": 10485760,
  "MESSAGE_QUEUE": {
    "301": "document_queue",
    "302": "localhost",
    "303": 5672
  }
